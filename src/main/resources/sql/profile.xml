<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="profile">
	<insert id="insert" parameterType="map">
		insert into PROFILE values(#{id} , #{profile.eng_name} , #{profile.profile_img_file} , #{profile.profile_img_ori} , #{profile.frontend} ,
							#{profile.backend}, #{profile.blog} , #{profile.job} , '0', #{profile.email} , #{profile.phone} , #{profile.github} , #{profile.selfintro} , #{profile.year},
							#{profile.posi} , #{profile.posi2})
	</insert>
	<select id="checkid" parameterType="String" resultType="int">
		select count(*) from PROFILE
	</select>


</mapper>